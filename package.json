{
  "name": "torrentdownloader",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "build": {
    "appId": "com.omg.asd.nani",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "productName": "torrentdownloader",
    "linux": {
      "target": "AppImage"
    }
  },
  "dependencies": {
    "electron-is-dev": "^1.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0",
    "react-spinners": "^0.8.0"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "electron-start": "electron --disable-gpu .",
    "electron-build": "electron-builder",
    "start": "concurrently --kill-others \"BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && npm run electron-start\"",
    "build": "npm run react-build && npm run electron-build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.0.2",
    "electron": "^7.1.7",
    "electron-builder": "^21.2.0",
    "eslint": "^6.8.0",
    "wait-on": "^3.3.0"
  }
}
